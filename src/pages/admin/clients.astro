---
import AdminLayout from "@/layouts/AdminLayout.astro"
import ApprovedClientsTable from "@/components/admin/tables/ApprovedClientsTable.vue";
import { authMiddleware } from "@/middleware/auth";
export const prerender = false;

const response = await authMiddleware(Astro, async () => {
  return new Response(null, {status: 200});
});

if (response instanceof Response && response.headers.get('Location')) {
  return Astro.redirect(response.headers.get('Location') || '/', response.status);
}
---


<AdminLayout title="TES - Clients">


    <ApprovedClientsTable titleCard="Contactos (Clientes aprobados)" client:only="vue"/>
</AdminLayout>
